    <Version>1.0.4</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Walgelijk" Version="0.13.3" />
    <PackageReference Include="Walgelijk.Onion" Version="0.4.2" />
    <PackageReference Include="Walgelijk.OpenTK" Version="0.11.4" />
    <PackageReference Include="Walgelijk.ParticleSystem" Version="0.2.11" />
    <PackageReference Include="Walgelijk.SimpleDrawing" Version="1.24.2" />
  </ItemGroup>
  <ItemGroup>
    <None Include="$(ProjectDir)resources\**" CopyToOutputDirectory="Always" />
  </ItemGroup>
</Project>